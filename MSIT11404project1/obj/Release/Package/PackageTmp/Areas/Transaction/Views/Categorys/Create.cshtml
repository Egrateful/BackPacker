@model MSIT11404project1.Models.Categorys

@using (Html.BeginForm("Create", "Categorys", new { area = "Transaction" }, FormMethod.Post, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
{
    <h3><strong>新增產品類別</strong></h3>
    <table class="table">
        <tr>
            <th>@Html.EditorFor(p => p.CategoryName, new { htmlAttributes = new { @class = "form-control" } })</th>
            <th><button type="submit" class="btn btn-default">新增類別</button></th>
        </tr>
    </table>


}